{
  "extends": "google",
  "env": {
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 6
  },
  "rules": {
    "comma-dangle": [2, "never"],
    "max-len": [2, {
      "code": 100,
      "tabWidth": 2
    }],
    "keyword-spacing": 2,
    "indent": [2, 4, { "SwitchCase": 1 }],
    "linebreak-style": ["error", "windows"]
  }
}